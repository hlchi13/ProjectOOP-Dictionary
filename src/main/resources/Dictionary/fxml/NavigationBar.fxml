<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<AnchorPane prefHeight="500.0" prefWidth="760.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.NavigationController">
   <children>
      <AnchorPane fx:id="container" layoutX="65.0" prefHeight="500.0" prefWidth="700.0" styleClass="background" stylesheets="@../../style.css" />
      <Pane prefHeight="500.0" prefWidth="65.0" styleClass="root">
         <children>
            <Button fx:id="mainDict" layoutX="10.0" layoutY="57.0" mnemonicParsing="false" prefHeight="46.0" prefWidth="46.0" styleClass="dictionary-btn" stylesheets="@../../style.css">
               <graphic>
                  <ImageView fitHeight="30.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../icons/icons8-search-40.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
            <Button fx:id="gameBtn" layoutX="10.0" layoutY="275.0" mnemonicParsing="false" prefHeight="46.0" prefWidth="46.0" styleClass="dictionary-btn" stylesheets="@../../style.css">
               <graphic>
                  <ImageView fitHeight="30.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../icons/icons8-game-40.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
            <Button fx:id="exitBtn" layoutX="10.0" layoutY="404.0" mnemonicParsing="false" prefHeight="46.0" prefWidth="46.0" styleClass="dictionary-btn" stylesheets="@../../style.css">
               <graphic>
                  <ImageView fitHeight="30.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../icons/icons8-log-out-40.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
            <Button fx:id="translateBtn" layoutX="10.0" layoutY="111.0" mnemonicParsing="false" prefHeight="46.0" prefWidth="46.0" styleClass="dictionary-btn" stylesheets="@../../style.css">
               <graphic>
                  <ImageView fitHeight="30.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../icons/icons8-translation-40.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
            <Button layoutX="10.0" layoutY="220.0" mnemonicParsing="false" onMouseClicked="#addWordButton" prefHeight="46.0" prefWidth="46.0" styleClass="dictionary-btn" stylesheets="@../../style.css">
               <graphic>
                  <ImageView fitHeight="30.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../icons/icons8-add-40.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
            <Button fx:id="bookmarkList" layoutX="10.0" layoutY="168.0" mnemonicParsing="false" prefHeight="46.0" prefWidth="46.0" styleClass="dictionary-btn">
               <graphic>
                  <ImageView fitHeight="30.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../icons/favorite.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
         </children>
      </Pane>
   </children>
</AnchorPane>
